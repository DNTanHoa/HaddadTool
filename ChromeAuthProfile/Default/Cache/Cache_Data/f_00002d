Page => {\n    setIsLoading(true);\n    const res = await onSearch(query, Page);\n    const { Code, Result } = res;\n    if (Code === 200) {\n      if (Page === 1) {\n        setItems(Result.Items);\n        if (Result.Items.length === 1) {\n          onSelect(Result.Items[0]);\n          setQuery('');\n          if (inGroup) {\n            setItems([]);\n            setTotal(0);\n          } else {\n            setIsOpen(false);\n            inputRef.current.blur();\n          }\n        } else {\n          setTotal(Result.Total);\n        }\n      } else {\n        setItems([...items, ...Result.Items]);\n      }\n    }\n    setIsLoading(false);\n  };\n\n  const search = () => {\n    setItems([]);\n    setTotal(0);\n    setIsOpen(true);\n    baseSearch(page);\n  };\n\n  const loadMore = () => {\n    const nextPage = page + 1;\n    setPage(nextPage);\n    baseSearch(nextPage);\n  };\n\n  const clearValue = () => {\n    setQuery('');\n    onSelect(null);\n    setItems([]);\n    setTotal(0);\n    inputRef.current.focus();\n  };\n\n  useEffect(() => {\n    const clickOutsideListener = e => {\n      if (elRef && elRef.current && !elRef.current.contains(e.target)) {\n        setIsOpen(false);\n      }\n    };\n    document.addEventListener('mousedown', e => clickOutsideListener(e));\n    return () => {\n      document.removeEventListener('mousedown', e => clickOutsideListener(e));\n    };\n  }, []);\n\n  return (\n    <div\n      ref={elRef}\n      className={classNames({\n        'searchable-dropdown': true,\n        disabled: disabled,\n      })}\n    >\n      <input\n        ref={inputRef}\n        className={classNames({ selected: !!selected })}\n        type=\"text\"\n        value={selected ? '' : query}\n        placeholder={placeholder}\n        onChange={e => {\n          setQuery(e.target.value);\n          setPage(1);\n        }}\n        onKeyDown={e => {\n          if (\n            !isLoading &&\n            query.length > 0 &&\n            e.key.toLowerCase() === 'enter'\n          ) {\n            e.preventDefault();\n            search();\n          }\n          if (e.key.toLowerCase() === 'escape') {\n            setIsOpen(false);\n            e.target.blur();\n          }\n          if (!isLoading && e.key.toLowerCase() === 'tab') {\n            if (query.length > 0) {\n              search();\n            }\n          }\n        }}\n        onFocus={() => {\n          if (!selected && items.length > 0) {\n            setIsOpen(true);\n          }\n        }}\n        onPaste={e => {\n          const paste = (e.clipboardData || window.clipboardData).getData(\n            'text',\n          );\n          setQuery(paste.trim());\n          e.preventDefault();\n        }}\n        onBlur={onBlur}\n      />\n      {selected && (\n        <SelectedItem\n          showClearButton={!disabled && !inGroup}\n          onClear={clearValue}\n        >\n          {children(selected)}\n        </SelectedItem>\n      )}\n      {!selected &&\n        !disabled && (\n          <div className=\"actions\">\n            <div\n              className={classNames({\n                search: true,\n                disabled: !query,\n              })}\n              onClick={search}\n            >\n              <img src={searchIco} alt=\"\" title=\"Search\" />\n            </div>\n          </div>\n        )}\n      {isOpen && (\n        <div\n          className=\"list\"\n          style={{\n            width: inputRef.current\n              ? `${inputRef.current.offsetWidth}px`\n              : 'auto',\n          }}\n        >\n          {getNotSelectedItems().map(item => {\n            return (\n              <div\n                key={item[keyField]}\n                className=\"item\"\n                onClick={() => {\n                  setQuery('');\n                  onSelect(item);\n                  setIsOpen(false);\n                  setItems([]);\n                  setTotal(0);\n                  if (inGroup) {\n            