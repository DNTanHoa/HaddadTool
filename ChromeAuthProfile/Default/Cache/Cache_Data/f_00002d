,\n  value,\n  touched,\n  error,\n  onChange,\n  onClear,\n  placeholder,\n  onBlur,\n  className,\n  views,\n  disabled,\n}) => {\n  const classes = classNames({\n    datepicker: true,\n    [className]: className,\n    'date-time-picker--error': touched && error,\n  });\n\n  return (\n    <div className={classes}>\n      <div className=\"datepicker-content\">\n        <DateTimePicker\n          min={min}\n          max={max}\n          value={value}\n          onChange={onChange}\n          onBlur={onBlur}\n          views={views || ['month', 'year', 'decade']}\n          placeholder={placeholder}\n          disabled={disabled}\n        />\n        {!!value &&\n          !disabled && <FieldClearButton onClear={() => {\n            if (!!onClear) {\n              onClear();\n            } else {\n              onChange(null);\n            }\n          }} />}\n      </div>\n      {touched &&\n        error && <span className=\"date-time-picker__error\">{error}</span>}\n    </div>\n  );\n};\n\nDatepicker.defaultProps = {\n  min: Date(1900, 0, 1),\n  max: Date(2099, 11, 31),\n  value: null,\n};\n\nDatepicker.propTypes = {\n  min: PropTypes.instanceOf(Date),\n  max: PropTypes.instanceOf(Date),\n  value: PropTypes.instanceOf(Date),\n  onChange: PropTypes.func.isRequired,\n};\n\nexport default Datepicker;\n"],"names":["useToast","showError","e","store","dispatch","error","getNotificationObj","toString","showSuccess","message","success","useModalVisible","keys","getDefaultModalVisibilityState","reduce","all","next","modalVisible","setModalVisible","useState","toggleModalVisible","data","prev","resetModalVisible","search","async","route","query","page","res","axios","appConfig","backendUrl","trim","Page","PageSize","Code","Result","searchFactories","searchReferences","searchColors","searchLabels","searchCustomers","searchCountries","useForm","onSubmit","currentValues","setCurrentValues","originalValues","setOriginalValues","setValues","newValues","values","setFieldValue","