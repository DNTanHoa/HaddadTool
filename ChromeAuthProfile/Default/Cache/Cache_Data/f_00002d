\n        const year = elements[1] || '';\n        let seasonName = '';\n        switch (elements[0]) {\n            case 'F': {\n                seasonName = 'Fall';\n                break;\n            }\n            case 'H': {\n                seasonName = 'Holiday';\n                break;\n            }\n            case 'M': {\n                seasonName = 'Summer';\n                break;\n            }\n            case 'S': {\n                seasonName = 'Spring';\n                break;\n            }\n        }\n        return [seasonName, year].join(' ');\n    }\n\n    const fetchControlSheet = async e => {\n        try {\n            e.preventDefault();\n            let po = poNumber;\n            if (!poNumber || poNumber.length < 6) {\n                return;\n            }\n            if (poNumber.length === 6 && !poNumber.startsWith('0')) {\n                po = `0${poNumber}`;\n            }\n            const controlSheet = await onFetch(po);\n            const factory = controlSheet.Factory?.split(' ')[0];\n            if (!factory) {\n                return;\n            }\n            const factoryResponse = await onFetchFactory(factory);\n            onFill({\n                ShipDates: [new Date(controlSheet.ShipDate)],\n                Colors: [controlSheet.Color],\n                Labels: [{\n                    Id: controlSheet.Label,\n                    Name: controlSheet.Label || 'BLANK'\n                }],\n                Reference: {\n                    Name: controlSheet.Ref,\n                    Id: controlSheet.Ref\n                },\n                Factory: factoryResponse.Result.Items[0],\n                Season: getSeason(controlSheet.Season),\n            }, {\n                Style: { Value: controlSheet.Style?.split(' ')[0] },\n                Po: { Po: po }\n            });\n            setPoNumber('');\n        } catch (err) {\n            showError(err);\n        }\n    }\n\n    if (!isVisible) {\n        return null;\n    }\n\n    return (\n        <form className=\"po-filler\" onSubmit={fetchControlSheet}>\n            <input\n                type=\"text\"\n                placeholder=\"Insert PO number to fill form data\"\n                value={poNumber}\n                onChange={e => setPoNumber(e.target.value?.trim())}\n            />\n            <Btn\n                htmlType=\"submit\"\n                type=\"primary\"\n                disabled={!poNumber}\n                onClick={e => e.target.blur()}\n            >\n                Fill form\n            </Btn>\n        </form>\n    );\n}\n\nexport default PoFiller\n","import GeneralInfo from './GeneralInfo';\n\nexport default GeneralInfo;","import React, { useEffect, useState } from 'react';\nimport { addYears, format } from 'date-fns';\nimport { uniq, uniqBy } from 'lodash';\nimport classNames from 'classnames';\nimport { v4 as uuidv4 } from 'uuid';\n\nimport Datepicker from '../common/components/Datepicker';\nimport SearchableDropdown, {\n  asRequest,\n  SearchableDropdownGroup,\n} from '../common/components/SearchableDropdown';\n\nimport { asDate, asDateWithoutTime } from '../common/utils/dateUtils';\nimport { useForm } from '../common/hooks/useForm';\nimport { useToast } from '../common/hooks/useToast';\nimport { useSeasonYear } from '../common/hooks/useSeasonYear';\n\nimport {\n  searchColors,\n  searchCustomers,\n  searchFactories,\n  searchLabels,\n  searchReferences,\n} from '../common/api/dictionariesApi';\n\nimport FlatForm, { Field } from '../common/components/FlatForm';\nimport TabForm from '../common/components/TabForm';\nimport DropdownList from '../common/components/dropdown/DropdownList';\nimport CheckboxList from '../common/components/CheckboxList';\n\nimport PoFiller from './components/PoFiller';\n\nimport './GeneralInfo.scss';\n\nconst GeneralInfo = React.forwardRef(\n  (\n    {\n      page,\n      defaultFactory,\n      request,\n      disabled,\n      onFetchPoNumbers,\n      onFetchStyles,\n      onFetchControlSheet,\n      onSave,\n    },\n    ref,\n  ) => {\n    const save = data => {\n      onSave({\n        Name: data.Name,\n        ReferenceNumber: data.Reference.Id,\n        Styles: data.Styles.map(item => item.Value),\n        Colors: data.Colors.map(item => item.Value),\n        Labels: data.Labels.map(item => item.Id),\n        CustomerId: data.Customer?.Account,\n        ShipDates: data.ShipDates.map(shipDate =>\n          asDateWithoutTime(shipDate.Date),\n        ),\n        AllPos: data.AllPos.map(item => item.Po),\n        Season: data.Season,\n        FactoryCode: data.Factory.Code,\n        StatusId: data.StatusId,\n      });\n    };\n\n    const [isFetching, setIsFetching] = useState({\n      poNumbers: false,\n      styles: false,\n    });\n    const [poNumbers, setPoNumbers] = useState([]);\n    const [availablePoNumbers, setAvailablePoNumbers] = useState([]);\n    const [styles, setStyles] = useState([]);\n\n    const [seasonYearValues] = useSeasonYear();\n\n    const { showError } = useToast();\n\n    const { values, setValues, setFieldValue, isEdited } = useForm({\n      onSubmit: save,\n    });\n\n    const fetchPoNumbers = async filterParams => {\n      try {\n        setIsFetching(prev => ({\n          ...prev,\n          poNumbers: true,\n        }));\n        const items = await onFetchPoNumbers(filterParams);\n        setPoNumbers(items);\n      } catch (e) {\n        showError(e);\n      } finally {\n        setIsFetching(prev => ({\n          ...prev,\n          poNumbers: false,\n        }));\n      }\n    };\n\n    const togglePoNumber = (item, checked) => {\n      if (checked) {\n        setFieldValue('AllPos', [...values.AllPos, item]);\n      } else {\n        setFieldValue('AllPos', values.AllPos.filter(po => po.Po !== item.Po));\n      }\n    };\n\n    const fetchStyles = async refNo => {\n      try {\n        setIsFetching(prev => ({\n          ...prev,\n          styles: true,\n        }));\n        const items = await onFetchStyles(refNo);\n        setStyles(items);\n      } catch (e) {\n        showError(e);\n      } finally {\n        setIsFetching(prev => ({\n          ...prev,\n          styles: false,\n        }));\n      }\n    };\n\n    const getDateUniqueValue = date => format(asDate(date), 'DD-MM-YYYY');\n\n    const addShipDate = date =>\n      setFieldValue(\n        'ShipDates',\n        uniqBy(\n          [\n            ...values.ShipDates,\n            {\n              Id: uuidv4(),\n              Date: date,\n              UniqueValue: getDateUniqueValue(date),\n            },\n          ],\n          'UniqueValue',\n        ),\n      );\n\n    const updateShipDate = (id, date) => {\n      const index = values.ShipDates.findIndex(shipDate => shipDate.Id === id);\n      setFieldValue(\n        'ShipDates',\n        uniqBy(\n          [\n            ...values.ShipDates.slice(0, index),\n            {\n              ...values.ShipDates[index],\n              Date: date,\n              UniqueValue: getDateUniqueValue(date),\n            },\n            ...values.ShipDates.slice(index + 1),\n          ],\n          'UniqueValue',\n        ),\n      );\n    };\n\n    const removeShipDate = id => {\n      setFieldValue(\n        'ShipDates',\n        values.ShipDates.filter(shipDate => shipDate.Id !== id),\n      );\n    };\n\n    const fillFormByRequest = (request, valuesToSelect) => {\n      setValues({\n        Name: request.Name,\n        Reference: request.Reference,\n        Styles: (request.Styles || []).map(Value => ({ Value })),\n        Colors: (request.Colors || []).map(Value => ({ Value })),\n        Labels: request.Labels,\n        Customer: request.Customer,\n        ShipDates: (request.ShipDates || []).map(shipDate => ({\n          Id: uuidv4(),\n          Date: asDate(shipDate),\n          UniqueValue: getDateUniqueValue(shipDate),\n        })),\n        AllPos: request.AllPos || [],\n        Season: seasonYearValues.find(\n          value => reques