          ...(values.AllPos || []),\n            values.ToSelect.Po,\n          ]));\n        }\n      },\n      [poNumbers],\n    );\n\n    useEffect(\n      () => {\n        if (!!values.Reference) {\n          fetchStyles(values.Reference.Id);\n          setStyles([]);\n        }\n      },\n      [values.Reference],\n    );\n\n    useEffect(\n      () => {\n        if (styles?.length === 1) {\n          setFieldValue('Styles', [styles[0]]);\n        }\n        if (values.ToSelect?.Style) {\n          setFieldValue('Styles', [values.ToSelect.Style]);\n        }\n      },\n      [styles, values.ToSelect],\n    );\n\n    const getVendorLabel = factory => {\n      if (!factory) {\n        return '';\n      }\n      if (!factory.VendorName) {\n        return factory.VendorCode;\n      }\n      return `${factory.VendorCode} / ${factory.VendorName}`;\n    };\n\n    const canSave = () => {\n      return (\n        !disabled &&\n        Object.keys(values)\n          .filter(key => !['ToSelect'].includes(key))\n          .every(key => {\n            if (\n              ['ShipDates', 'AllPos', 'Colors', 'Labels', 'Styles'].includes(\n                key,\n              )\n            ) {\n              return values[key].length > 0;\n            }\n            return !!values[key];\n          })\n      );\n    };\n\n    return (\n      <TabForm\n        ref={ref}\n        mainPath={page}\n        values={values}\n        isEdited={isEdited()}\n        canSave={canSave()}\n        onSave={save}\n      >\n        <form className=\"info\">\n          <div\n            className={classNames({\n              header: true,\n            })}\n          >\n            <FlatForm>\n              <Field name=\"Name\" disabled={disabled} readonly>\n                {values.Name}\n              </Field>\n            </FlatForm>\n          </div>\n          <div className=\"columns\">\n            <div className=\"first\">\n              <FlatForm>\n                <Field name=\"Season\">\n                  <DropdownList\n                    data={seasonYearValues}\n                    value={values.Season}\n                    filter=\"contains\"\n                    disabled={disabled}\n                    onChange={selection => setFieldValue('Season', selection)}\n                  />\n                </Field>\n                <Field name=\"Factory\">\n                  <SearchableDropdown\n                    keyField=\"Code\"\n                    disabled={disabled || !!defaultFactory}\n                    selected={values.Factory}\n                    placeholder=\"Search for factory\"\n                    onSearch={searchFactories}\n                    onSelect={item => setFieldValue('Factory', item)}\n                  >\n                    {item => (\n                      <span>\n                        {item.Code} / {item.Name}\n                      </span>\n                    )}\n                  </SearchableDropdown>\n                </Field>\n                <Field name=\"Managing Office\" disabled={disabled} readonly>\n                  {getVendorLabel(values.Factory)}\n                </Field>\n                <Field name=\"Country\" disabled={disabled} readonly>\n                  {values.Factory ? values.Factory.Country : ''}\n                </Field>\n                <Field name=\"Reference Number\">\n                  <SearchableDropdown\n                    disabled={disabled}\n                    selected={values.Reference}\n                    placeholder=\"Search for reference\"\n                    onSearch={searchReferences}\n                    onSelect={item => setFieldValue('Reference', item)}\n                  >\n                    {item => <span>{item.Name}</span>}\n                  </SearchableDropdown>\n                </Field>\n                <Field name=\"Styles\" multi>\n                  <SearchableDropdownGroup\n                    onSearch={query =>\n                      asRequest(\n                   