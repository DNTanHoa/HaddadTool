.to),\n    };\n  };\n\n  const [dateFilters, setDateFilters] = useState(getDateFilters());\n\n  useEffect(\n    () => {\n      setDateFilters(getDateFilters());\n    },\n    [JSON.stringify(filterParams.CreatedDate)],\n  );\n\n  const onFiltersChange = (newFilterParams, newSortParams = null) => {\n    requestsActions.fetchItems(\n      pageParams,\n      newSortParams || sortParams,\n      newFilterParams,\n    );\n  };\n\n  const getNewDateFilters = (key, value) => {\n    return {\n      ...dateFilters,\n      [key]: value,\n    };\n  };\n\n  const applyDateFilters = (newFilters, onMoreFiltersChange) => {\n    setDateFilters(newFilters);\n    onMoreFiltersChange({\n      ...filterParams,\n      CreatedDate: {\n        from: newFilters.createdDateFrom,\n        to: newFilters.createdDateTo,\n      },\n    });\n  };\n\n  const { isFactory, isAdmin, isMultiApprover } = usePermission();\n\n  return (\n    <ListFilters\n      module=\"requests\"\n      className=\"requests-filters\"\n      filtersPlaceholder=\"Requests\"\n      filterParams={filterParams}\n      sortParams={sortParams}\n      defaultFilterParams={getDefaultFiltersParams()}\n      searchPlaceholder=\"Name / Approved by\"\n      searchFieldName=\"Text\"\n      showSelectedFilters\n      selectedFiltersLabelsMap={{\n        Text: 'Search:',\n        'Customer.Account': 'Customer:',\n        'Factory.Code': 'Factory:',\n        'Factory.Country': 'Country:',\n        Label: 'Label:',\n        Season: 'Season / Year:',\n        CreatedDate: 'Created',\n      }}\n      showFilterCode\n      onFiltersChange={onFiltersChange}\n      onClearMoreFilters={() => {\n        setDateFilters({\n          CreatedDate: {\n            from: null,\n            to: null,\n          },\n        });\n      }}\n    >\n      {({\n        onSingleValueFilterChange,\n        onMultiValueFilterChange,\n        onClearMutliValueFilter,\n        onMoreFiltersChange,\n      }) => (\n        <>\n          <SearchableDropdownFilter\n  