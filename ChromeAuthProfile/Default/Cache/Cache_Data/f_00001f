Label} {valueName}</div>}\n            {!!groupType && !!groupName && <div>on <b>{groupType}/{groupName}</b></div>}\n            <div className=\"change\">\n                {!isNewValue &&\n                    <>\n                        <div className=\"old\">{oldValue}</div>\n                        <FontAwesomeIcon icon={['fas', 'long-arrow-alt-right']} />\n                    </>\n                }\n                <div className=\"new\">{newValue}</div>\n            </div>\n        </div>\n    );\n}\n\nexport default HistoryEventSingleValue;\n","import React, { useEffect, useState } from 'react';\nimport difference from 'lodash/difference';\n\nimport HistoryEventSingleValue from './HistoryEventSingleValue';\n\nconst HistoryEventDifference = ({\n    changeName,\n    oldValue,\n    newValue,\n    fieldsMap = {},\n    fieldParser = {}\n}) => {\n    const [diff, setDiff] = useState({});\n\n    const parser = (value, field) => {\n        if (fieldParser[field]) {\n            return fieldParser[field](value);\n        }\n        return value;\n    }\n\n    const generateDifference = () => {\n        return Object.keys(newValue).reduce((diff, nextKey) => {\n            if (Array.isArray(newValue[nextKey])) {\n                if (difference(newValue[nextKey], oldValue[nextKey]).length > 0) {\n                    diff[nextKey] = {\n                        old: !oldValue[nextKey]?.length ? null : oldValue[nextKey].map(v => parser(v, nextKey)).join('; '),\n                        new: !newValue[nextKey]?.length ? null : newValue[nextKey].map(v => parser(v, nextKey)).join('; ')\n                    }\n                }\n            } else if (newValue[nextKey] !== '' && newValue[nextKey] !== oldValue[nextKey]) {\n                diff[nextKey] = {\n                    old: parser(oldValue[nextKey], nextKey),\n                    new: parser(newValue[nextKey], nextKey)\n                }\n            }\n            return diff;\n        }, {});\n    }\n\n    useEffect(() => {\n        setDiff(generateDifference());\n    }, [oldValue, newValue]);\n\n    return (\n        <div className=\"history-event-difference\">\n            {!!changeName && <div>Updated <b>{changeName}</b></div>}\n            {Object.keys(diff).map(key =>\n                <div className=\"field\" key={key}>\n                    <div className=\"field-name\">{fieldsMap[key] || key}:</div>\n                    <HistoryEventSingleValue\n                        oldValue={diff[key]?.old}\n                        newValue={diff[key]?.new}\n                    />\n                </div>\n            )}\n        </div>\n    );\n}\n\nexport default HistoryEventDifference\n","import React, { useEffect, useState } from 'react';\nimport HistoryEventDifference from './HistoryEventDifference';\n\nconst HistoryEventCartons = ({\n    oldValue,\n    newValue,\n    fieldsMap,\n}) => {\n\n    const [data, setData] = useState({});\n\n    const groupByPo = value => {\n        return value.reduce((value, nextItem) => {\n            value[nextItem.Po] = nextItem;\n            delete value[nextItem.Po].Id;\n            delete value[nextItem.Po].Po;\n            return value;\n        }, {});\n    }\n\n    const generateData = () => {\n        const oldPerPo = groupByPo(oldValue);\n        const newPerPo = groupByPo(newValue);\n\n        return Object.keys(newPerPo).reduce((diff, nextKey) => {\n            diff[nextKey] = {\n                old: oldPerPo[nextKey],\n                new: newPerPo[nextKey]\n            }\n            return diff;\n        }, {})\n    }\n\n    useEffect(() => {\n        setData(generateData());\n    }, [oldValue, newValue]);\n\n    return (\n        <div className=\"history-event-cartons\">\n            {Object.keys(data).map(key =>\n                <div key={key} className=\"carton\">\n                    <div>{!data[key].old ? 'Added' : 'Updated'} closed carton data for <b>{key}</b></div>\n                    <HistoryEventDifference\n                        oldValue={data[key]?.old || {}}\n                        newValue={data[key]?.new || {}}\n                        fieldsMap={fieldsMap}\n                    />\n                </div>\n            )}\n        </div>\n    );\n}\n\nexport default HistoryEventCartons;\n","import React from 'react';\nimport classNames from 'classnames';\n\nconst HistoryEventAddDeleteComment = ({\n    code,\n    value\n}) => {\n    return (\n        <div className=\"history-event-add-delete-comment\">\n            {code === 'ADD_COMMENT' ? 'Add' : 'Delete'} <b>Comment</b>\n            <div className={classNames({\n                [code === 'ADD_COMMENT' ? 'add' : 'delete']: true\n            })}>\n                {!!value.Text && <div>Text: {value.Text}</div>}\n                {!!value.Attachments.length && <div>Attachments: {value.Attachments.join(', ')}</div>}\n            </div>\n        </div>\n    );\n}\n\nexport default HistoryEventAddDeleteComment;\n","import HistoryModal from './HistoryModal';\n\nexport default HistoryModal;","import React, { useEffect, useState } from 'react';\nimport { format } from 'date-fns';\n\nimport ModalBody from 'react-bootstrap/lib/ModalBody';\n\nimport ModalBase from '../common/components/ModalBase';\nimport ModalHeader from '../common/components/ModalHeader';\nimport ModalButtons from '../common/components/modal/ModalButtons';\n\nimport CurtainedSpinner from '../common/components/CurtainedSpinner';\n\nimport { useToast } from '../common/hooks/useToast';\nimport { useDictionaryByCode } from '../common/hooks/useDictionaryByCode';\n\nimport HistoryEventHeader from './components/HistoryEventHeader';\nimport HistoryEventAttachment from './components/HistoryEventAttachment';\nimport HistoryEventSingleValue from './components/HistoryEventSingleValue';\nimport HistoryEventDifference from './components/HistoryEventDifference';\nimport HistoryEventCartons from './components/HistoryEventCartons';\nimport HistoryEventAddDeleteComment from './components/HistoryEventAddDeleteComment';\n\nimport './HistoryModal.scss';\n\n\nconst HistoryModal = ({\n    show,\n    hide,\n    id,\n    dictionaries,\n    onFetch\n}) => {\n\n\n    const [isFetching, setIsFetching] = useState(false);\n    const [items, setItems] = useState([]);\n\n    const [getGroupType] = useDictionaryByCode(dictionaries?.groupTypes, { codeField: 'Id', nameField: 'Name' });\n    const [getGroupName] = useDictionaryByCode(dictionaries?.groupNames, { codeField: 'Id', nameField: 'Name' });\n\n    const { showError } = useToast();\n\n    const isObjectOrArray = value => value?.startsWith('{') || value?.startsWith('[');\n\n    const fetchHistory = async () => {\n        try {\n            setIsFetching(true);\n            const fetchedItems = await onFetch(id);\n            setItems(fetchedItems.map(item => ({\n                ...item,\n                OldValue: isObjectOrArray(item.OldValue) ? JSON.parse(item.OldValue) : item.OldValue,\n                NewValue: isObjectOrArray(item.NewValue) ? JSON.parse(item.NewValue) : item.NewValue\n            })));\n        }\n        catch (err) {\n            showError(err);\n        } finally {\n            setIsFetching(false);\n        }\n    };\n\n    const getEventDetails = item => {\n        switch (item.EventType.Code) {\n            case 'ADD_ATTACHMENT':\n            case 'DELETE_ATTACHMENT': {\n                return <HistoryEventAttachment\n                    code={item.EventType.Code}\n                    groupType={getGroupType(item.NewValue.TypeId)}\n                    groupName={(getGroupName(item.NewValue.NameId))}\n                    attachments={item.NewValue.Attachments || [item.NewValue.Attachment]}\n                />;\n            }\n            case 'UPDATE_GROUP_COMMENT': {\n                return <HistoryEventSingleValue\n                    addLabel=\"Added\"\n                    updateLabel=\"Updated\"\n                    valueName=\"group comment\"\n                    groupType={getGroupType(item.NewValue.TypeId)}\n                    groupName={(getGroupName(item.NewValue.NameId))}\n                    oldValue={item.OldValue.Comment}\n                    newValue={item.NewValue.Comment}\n                />;\n            }\n            case 'CHANGE_STATUS': {\n                return <HistoryEventSingleValue\n                    updateLabel=\"Changed\"\n                    valueName=\"status\"\n                    oldValue={item.OldValue}\n                    newValue={item.NewValue}\n                />;\n            }\n            case 'UPDATE_SO_FORM': {\n                return <HistoryEventSingleValue\n                    addLabel=\"Set\"\n                    valueName=\"SO Form\"\n                    oldValue={item.OldValue}\n                    newValue={item.NewValue}\n                />;\n            }\n            case 'UPDATE_GENERAL_INFO': {\n                return <HistoryEventDifference\n                    changeName=\"General Info\"\n                    oldValue={item.OldValue}\n                    newValue={item.NewValue}\n                    fieldsMap={{\n                        AllPos: 'All POs',\n                        ShipDates: 'Ship Dates',\n                        FactoryCode: 'Factory',\n                        CustomerId: 'Customer',\n                        ReferenceNumber: 'Reference #'\n                    }}\n                    fieldParser={{\n                        Labels: value => value === '' ? 'BLANK' : value,\n                        ShipDates: value => format(new Date(value), 'MMM D, GGGG')\n                    }}\n                />;\n            }\n            case 'UPDATE_CARTONS': {\n                return <HistoryEventCartons\n                    oldValue={item.OldValue}\n                    newValue={item.NewValue}\n                    fieldsMap={{\n                        NetWeight: 'Net Weight',\n                        GrossWeight: 'Gross Weight'\n                    }}\n                />;\n            }\n\n            case 'ADD_COMMENT':\n            case 'DELETE_COMMENT': {\n                return <HistoryEventAddDeleteComment\n                    code={item.EventType.Code}\n                    value={item.NewValue}\n                />;\n            }\n\n            case 'UPDATE_COMMENT': {\n                return <HistoryEventDifference\n                    changeName=\"Comment\"\n                    oldValue={item.OldValue}\n                    newValue={item.NewValue}\n                />;\n            }\n\n            default: return <>Created request</>\n        }\n    }\n\n    useEffect(() => {\n        if (!!id) {\n            fetchHistory();\n        } else {\n            setItems([]);\n        }\n    }, [id])\n\n    return (\n        <ModalBase show={show} onHide={hide} className=\"history-modal\">\n            <ModalHeader\n                title=\"History\"\n                handleToggle={hide}\n            />\n            <ModalBody>\n                <div className=\"history-table\">\n                    <CurtainedSpinner\n                        isVisible={isFetching}\n                        size={100}\n                    />\n                    {!isFetching && !items.length &&\n                        <div className=\"no-history\">There is no history for this request.</div>\n                    }\n                    {items.map(item => (\n                        <div key={item.Id} className=\"history-row\">\n                            <HistoryEventHeader item={item} />\n                            <div className=\"details\">\n                                {getEventDetails(item)}\n                            </div>\n                        </div>\n                    ))}\n                </div>\n            </ModalBody>\n            <ModalButtons\n                canBeApproved={false}\n                labelCancel=\"close\"\n                handleCancel={hide}\n            />\n        </ModalBase>\n    );\n};\n\nexport default HistoryModal;\n","import Re