 e => {\n      if (isShiftDown) {\n        e.preventDefault();\n      }\n    },\n    [isShiftDown],\n  );\n\n  const isItemChecked = item =>\n    listState.checked\n      .map(checkedItem => checkedItem[idField])\n      .includes(item[idField]);\n\n  const isAllItemsChecked = () => {\n    return (\n      listState.checked.length > 0 &&\n      difference(\n        listState.items.map(item => item[idField]),\n        listState.checked.map(item => item[idField]),\n      ).length === 0\n    );\n  };\n\n  const getNewSelectedItems = value => {\n    const currentSelectedIndex = listState.items.findIndex(\n      item => item === value,\n    );\n\n    const lastSelectedIndex = listState.items.findIndex(\n      item => item === lastSelectedItem,\n    );\n\n    return listState.items.slice(\n      Math.min(lastSelectedIndex, currentSelectedIndex),\n      Math.max(lastSelectedIndex, currentSelectedIndex) + 1,\n    );\n  };\n\n  const getNextValue = value => {\n    const hasBeenSelected = !listState.checked.includes(value);\n\n    if (isShiftDown) {\n      const newSelectedItems = getNewSelectedItems(value);\n      // de-dupe the array using a Set\n      let selections = [\n        ...new Set([...listState.checked, ...newSelectedItems]),\n      ];\n\n      if (!hasBeenSelected) {\n        selections = [...newSelectedItems];\n      }\n\n      return selections;\n    }\n\n    // if it's already in there, remove it, otherwise append it\n    return listState.checked.includes(value)\n      ? listState.checked.filter(item => item !== value)\n      : [...listState.checked, value];\n  };\n\n  const toggleCheckItem = item => {\n    const check = !isItemChecked(item);\n    let nextValue;\n    if (isAllItemsChecked() && !check && isShiftDown) {\n      nextValue = [item];\n    } else {\n      nextValue = isShiftDown ? getNextValue(item) : [item];\n    }\n    setLastSelectedItem(item);\n\n    listActions.toggleCheckItems(nextValue, check);\n  };\n\n  const toggleCheckAll = check => {\n    l