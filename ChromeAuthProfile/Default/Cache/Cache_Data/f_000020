'form-control': true,\n    [inputClasses]: inputClasses,\n    error,\n  });\n\n  const btnClass = classNames({\n    'cell-disabled': disabled,\n    [buttonClasses]: buttonClasses,\n  });\n\n  useEffect(\n    () => {\n      setTextValue(double ? `${value}` : value);\n    },\n    [value],\n  );\n\n  const handleChange = event => {\n    setTextValue(event.target.value);\n  };\n\n  const handleBlur = () => {\n    if (allowEmpty || textValue !== originalValue) {\n      const isUnique = uniqueValues ? !uniqueValues.includes(textValue) : true;\n      const validByCustomValidator = validate ? validate(textValue) : true;\n      const isValid = allowEmpty\n        ? isUnique && validByCustomValidator\n        : !!textValue && isUnique && validByCustomValidator;\n      setError(!isValid);\n      if (isValid) {\n        onChange(double ? parseFloat(textValue) : textValue);\n        setIsEditing(state => !state);\n      }\n    } else {\n      setError(true);\n      setIsEditing(state => !state);\n    }\n  };\n\n  const handleClick = () => {\n    setOriginalValue(textValue);\n    setIsEditing(state => !state);\n  };\n\n  const handleKeyDown = e => {\n    if (e.keyCode === KeyCodes.ENTER) {\n      inputRef.blur();\n    } else if (e.keyCode === KeyCodes.ESC) {\n      setTextValue(originalValue);\n      setError(false);\n      setIsEditing(state => !state);\n    }\n  };\n\n  const getTitle = () => {\n    if (value) {\n      return textValue || value;\n    }\n\n    return placeholder;\n  };\n\n  const getEditor = () => {\n    if (double) {\n      return (\n        <DoubleInput\n          value={textValue}\n          onChange={newValue => setTextValue(newValue)}\n          onBlur={handleBlur}\n          autoFocus\n        />\n      );\n    }\n    return (\n      <input\n        ref={e => (inputRef = e)}\n        type={type}\n        className={inputClass}\n        value={textValue}\n        min={min}\n        maxLength={maxLength}\n        autoFocus={autoFocus}\n        autoComplete={