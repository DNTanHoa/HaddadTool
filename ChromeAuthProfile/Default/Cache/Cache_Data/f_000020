e),\n      },\n    });\n  };\n\n  const onClearMutliValueFilter = fieldName => {\n    applyAndSaveFilters({\n      ...filterParams,\n      [fieldName]: [],\n    });\n  };\n\n  const onClearFilters = () => {\n    if (onClearMoreFilters) {\n      onClearMoreFilters();\n    }\n    applyAndSaveFilters(defaultFilterParams);\n  };\n\n  const onSearchChange = value => {\n    onSingleValueFilterChange(value, searchFieldName);\n  };\n\n  const [modalOpen, setModalOpen] = useState(false);\n  const [setAlreadyExists, setSetAlreadyExists] = useState(false);\n  const [name, setName] = useState('');\n\n  useEffect(() => {\n    if (module) {\n      filtersSetsActions.getFiltersSetForModule(module);\n    }\n  }, []);\n\n  const onSelectFilters = filtersSetName => {\n    filtersSetsActions.setFilters(module, filtersSetName);\n    onFiltersChange(\n      filtersSets[filtersSetName].Filter,\n      filtersSets[filtersSetName].Sort,\n    );\n  };\n\n  const onClearSelect = () => {\n    filtersSetsActions.setFilters(module, null);\n    onClearFilters();\n  };\n\n  const onRemoveFilters = (filtersSetName, filtersSetId) => {\n    filtersSetsActions.removeFiltersSet(module, filtersSetName, filtersSetId);\n    if (chosenSet === filtersSetName) {\n      onClearSelect();\n    }\n  };\n\n  const toggleModal = () => {\n    setModalOpen(!modalOpen);\n    setSetAlreadyExists(false);\n    setName('');\n  };\n\n  const saveCurrentFilters = () => {\n    filtersSetsActions.saveFilter(module, name, filterParams, sortParams);\n    toggleModal();\n  };\n\n  const onSaveFilters = () => {\n    if (Object.keys(filtersSets).includes(name)) {\n      setSetAlreadyExists(true);\n    } else {\n      saveCurrentFilters();\n    }\n  };\n\n  const isFiltersEmpty = isEqual(defaultFilterParams, filterParams);\n  const isFilterSelected = Boolean(chosenSet);\n\n  return (\n    <section className={sectionClassName}>\n      {module && (\n        <>\n          <FiltersSavedOuter>\n            {isFilterSelected && <span>Sa